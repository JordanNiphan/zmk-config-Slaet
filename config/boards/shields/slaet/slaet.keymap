/ {
    macros {
        //keybinding is <&FakeLastTest_123>
        FakeLastTest_123: FakeLastTest_123 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp 1 &kp 2>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp T>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp E &kp S &kp T &kp I &kp N &kp G &kp 3 &kp 4>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp A &kp L &kp L>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp T &kp H &kp E>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp D &kp E &kp V &kp I &kp C &kp E &kp S>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp  &kp 1 &kp 1 &kp EXCL &kp EXCL &kp EXCL>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp D>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp EXCL>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp A>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp AT>
                ;
            };
        //keybinding is <&FakeName_01>
        FakeName_01: FakeName_01 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LSHFT> 
                , <&macro_tap &kp T>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp E &kp S &kp T &kp I &kp N &kp G>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp F>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp K &kp E>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp N>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp M &kp E &kp 1 &kp SPACE &kp EXCL &kp EXCL &kp EXCL &kp SPACE>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp F>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp K &kp E>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp L>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp S &kp T &kp N &kp A &kp M &kp E>
                ;
            };
        //keybinding is <&Slaet_Test_Macro>
        Slaet_Test_Macro: Slaet_Test_Macro {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LSHFT> 
                , <&macro_tap &kp T>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp E &kp S &kp T &kp I &kp N &kp G &kp 1 &kp 2 &kp 3 &kp 4 &kp 5 &kp 6 &kp 7 &kp 8 &kp 9 &kp SPACE &kp EXCL &kp AT &kp HASH &kp DLLR &kp PRCNT &kp CARET &kp AMPS &kp STAR &kp LPAR &kp SPACE>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp T &kp E &kp S &kp T>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp BSLH &kp BSLH &kp BSLH &kp SPACE>
                , <&macro_press &kp LSHFT> 
                , <&macro_tap &kp T &kp E &kp S &kp T>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp EXCL &kp EXCL &kp EXCL>
                ;
            };
        //keybinding is <&Test1>
        Test1: Test1 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LSHFT> 
                , <&macro_tap &kp T>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp E &kp S &kp T &kp 1>
                ;
            };
        //keybinding is <&Test2>
        Test2: Test2 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp 1 &kp 2 &kp 3 &kp 4 &kp 5 &kp 6 &kp 7 &kp 8 &kp 9 &kp EXCL &kp AT &kp HASH &kp DLLR &kp PRCNT &kp CARET &kp AMPS &kp STAR &kp LPAR &kp BSLH>
                ;
            };
        //keybinding is <&Test5>
        Test5: Test5 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp N &kp O>
                ;
            };
        //keybinding is <&test6>
        test6: test6 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp E>
                ;
            };
        };
      
    combos {
        compatible = "zmk,combos";
        combo_default {
            timeout-ms = <50>;
            key-positions = <46 60>;
            bindings = <&to DEFAULT>;
        };        
        combo_esc {
            timeout-ms = <50>;
            key-positions = <0 1>;
            bindings = <&kp ESC>;
        };
        combo_ent {
            timeout-ms = <50>;
            key-positions = <55 56>;
            bindings = <&kp RET>;
        };
        combo_bspc {
            timeout-ms = <50>;
            key-positions = <41 42>;
            bindings = <&kp BSPC>;
        };
        combo_tab {
            timeout-ms = <50>;
            key-positions = <17 18>;
            bindings = <&kp TAB>;
        };
        combo_quote {
            timeout-ms = <50>;
            key-positions = <23 24>;
            bindings = <&kp SQT>;
        };
        combo_lclk {
            timeout-ms = <50>;
            key-positions = <31 32>;
            bindings = <&mkp LCLK>;
        };
        combo_rclk {
            timeout-ms = <50>;
            key-positions = <30 31>;
            bindings = <&mkp RCLK>;
        };
        combo_del {
            timeout-ms = <50>;
            key-positions = <49 50>;
            bindings = <&kp DEL>;
        };
        combo_qwerty {
            timeout-ms = <50>;
            key-positions = <11 12 13>;
            bindings = <&to QWERTY>;
        };    
        combo_btclrall {
            timeout-ms = <50>;
            key-positions = <0 14 28>;
            bindings = <&bt BT_CLR_ALL>;            
        };
        combo_bttog {
            timeout-ms = <50>;
            key-positions = <36 52 67>;
            bindings = <&out OUT_TOG>;            
        };
    };
};

